{
    "manifest_version": 2,
    "name": "Codementum by Turtle Team 11",
	"version": "1.0",

	"description": "Momentum Extension Clone by Turtle Team 11 as part of Chingu-Voyage Cohort.",
	
	"browser_action": { 
		"default_icon": "img/logo_19x19.png"
	},

	"content_scripts": [
		{
		"matches": ["<all_urls>"],
		"js": ["js/jquery-3.2.1.slim.js", "js/script.js"]
		}
	],

	"chrome_url_overrides": {
      "newtab": "codementum.html"
    },

	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	],

	"background": {
		"scripts": ["js/background.js"]
	},
	
	"icons": {
		"64": "img/logo_64x64.png",
		"32": "img/logo_32x32.png"
	}

}